{
  "ErrorDocument": {
    "Key": "404.html"
  },
  "IndexDocument": {
    "Suffix": "index.html"
  },
  "RoutingRules": [
    {
      "Condition": {
        "KeyPrefixEquals": "index.html"
      },
      "Redirect": {
        "HttpRedirectCode": "307",
        "ReplaceKeyWith": "maintenance.html"
      }
    }
  ]
}
